<?xml version="1.0" encoding="utf-8"?>
<resources>
  <style name="AttachPickerLabel">
    <item name="android:ellipsize">end</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_width">wrap_content</item>
    <item name="android:maxLines">1</item>
    <item name="android:singleLine">true</item>
    <item name="android:textColor">#ffffffff</item>
  </style>
  <style name="Platform.AppCompat.Light" parent="@android:style/Theme.Material">
    <item name="android:backgroundTint">#ff000000</item>
    <item name="android:colorPrimary">#ff075e54</item>
    <item name="android:colorPrimaryDark">#ff000000</item>
    <item name="android:navigationBarColor">#ff000000</item>
    <item name="android:textColor">#fffafafa</item>
    <item name="android:textColorPrimary">#fffafafa</item>
    <item name="android:textColorSecondary">#dd888888</item>
    <item name="android:windowActionBar">false</item>
    <item name="android:windowNoTitle">true</item>
  </style>
  <style name="Toolbar" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar">
    <item name="android:backgroundTint">#ff075e54</item>
  </style>
</resources>